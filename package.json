{
  "name": "motepair-vscode",
  "displayName": "Motepair",
  "description": "Pair programming tool",
  "version": "0.0.1",
  "publisher": "drew-wallace",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.highlight",
    "onCommand:extension.refresh",
    "onCommand:extension.edit"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.highlight",
        "title": "Highlight"
      },
      {
        "command": "extension.refresh",
        "title": "Refresh"
      },
      {
        "command": "extension.edit",
        "title": "Edit"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "vscode": "^0.11.0"
  },
  "dependencies": {
    "copy-paste": "^1.3.0",
    "crypto": "0.0.3"
  }
}
